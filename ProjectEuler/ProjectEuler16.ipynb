      "def power_digit_sum(n):\n",
      "    number = str(2**n)\n",
      "    total = 0\n",
      "    for i in range(len(number)):\n",
      "        digit = number[i:i+1]\n",
      "        total = total + int(digit)\n",
      "    return total\n",
