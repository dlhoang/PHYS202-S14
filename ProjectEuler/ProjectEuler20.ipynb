     "input": [
      "def factorial_digit_sum(n):\n",
      "    value = n\n",
      "    total = 0\n",
      "    for i in range(1,n):\n",
      "        value = value * (n-i)\n",
      "    string = str(value)\n",
      "    for digit in string:\n",
      "        total = total + int(digit)\n",
      "    return total\n",
      "\n",
      "print factorial_digit_sum(10)\n",
      "print factorial_digit_sum(100)"
     ],
